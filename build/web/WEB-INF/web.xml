<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

<servlet>
  <servlet-name>RegisterServlet</servlet-name>
  <servlet-class>com.smarttour.controller.RegisterServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>RegisterServlet</servlet-name>
  <url-pattern>/RegisterServlet</url-pattern>
</servlet-mapping>

<servlet>
  <servlet-name>LoginServlet</servlet-name>
  <servlet-class>com.smarttour.controller.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>LoginServlet</servlet-name>
  <url-pattern>/LoginServlet</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>HelloDBServlet</servlet-name>
    <servlet-class>com.smarttour.controller.HelloDBServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>HelloDBServlet</servlet-name>
    <url-pattern>/HelloDBServlet</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>AdminDashboardServlet</servlet-name>
    <servlet-class>com.smarttour.controller.AdminDashboardServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>AdminDashboardServlet</servlet-name>
    <url-pattern>/admin/dashboard</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>AdminUserListServlet</servlet-name>
    <servlet-class>com.smarttour.controller.AdminUserListServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>AdminUserListServlet</servlet-name>
    <url-pattern>/AdminUserListServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>SearchTourServlet</servlet-name>
    <servlet-class>com.smarttour.controller.SearchTourServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>SearchTourServlet</servlet-name>
    <url-pattern>/SearchTourServlet</url-pattern>
</servlet-mapping>

</web-app>


